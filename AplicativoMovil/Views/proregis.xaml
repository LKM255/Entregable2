<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AplicativoMovil.Views.proregis">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="nombre" PlaceholderColor="Black" x:Name="nombre"/>
            <Entry Placeholder="precio" PlaceholderColor="Black" x:Name="precio"/>
            <Entry Placeholder="imagen" PlaceholderColor="Black" x:Name="imagen"/>
            <Entry Placeholder="descripcion" PlaceholderColor="Black" x:Name="descrip"/>
            <Button Text="Iniciar Sesion" BackgroundColor="Black" TextColor="White" CornerRadius="20" Clicked="Button_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>